//- This is the program element
div.list-item.row.row-space-between(ng-click='goto({toType: "program", toId: id, fromName: from})')
  span
    | {{ name }}
  .row
    button.m-right-2
      img(src="images/edit.svg")
    button(ng-click='deleteProgram(id, $event)')
      img(src="images/remove.svg")
